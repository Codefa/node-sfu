  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/lodash": "^4.14.161",
    "@types/node": "^14.0.1",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^26.0.0",
    "ts-jest": "^26.0.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
