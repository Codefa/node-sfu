{
  "name": "node-sfu",
  "version": "0.0.1",
  "keywords": [
    "WebRTC",
    "SFU",
    "node.js"
  ],
  "description": "RTP,RTCP,SRTP,SRTCP implementation for TypeScript.",
  "homepage": "https://github.com/shinyoshiaki/node-sfu",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shinyoshiaki/node-sfu.git"
  },
  "license": "MIT",
  "author": {
    "name": "shinyoshiaki"
  },
  "main": "./lib/index.js",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "ci": "yarn type && yarn lint && yarn test",
    "client": "cd demo/client && yarn dev",
    "deploy": "./build.sh && yarn publish",
    "fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "lint": "eslint src/**/*.ts",
    "server": "cd demo/server && yarn dev",
    "test": "yarn type && jest --forceExit",
    "type": "tsc --noEmit -p .",
    "unused": "organize-imports-cli src/**/*.ts"
  },
  "dependencies": {
    "lodash": "^4.17.20",
    "uuid": "^8.3.0",
    "werift": "^0.0.26"
  },
  "devDependencies": {
    "@types/jest": "^26.0.0",
    "@types/lodash": "^4.14.161",
    "@types/node": "^14.0.1",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^26.0.0",
    "organize-imports-cli": "^0.7.0",
    "prettier": "^2.0.5",
    "ts-jest": "^26.0.0",
    "ts-node": "^8.9.1",
    "typescript": "^3.9.2"
  },
  "engines": {
    "node": ">=14"
  },
  "readme": "README.md"
}
